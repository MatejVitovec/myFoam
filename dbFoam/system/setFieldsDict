FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


regions
(
    boxToCell
    {
        box (5 0 0) (10 0.1 0.1);
        fieldValues
        (
            volVectorFieldValue U.1     (100 0 0)
            volVectorFieldValue U.2     (100 0 0)
        );
    }
);


