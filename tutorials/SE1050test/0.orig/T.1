FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      T.1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 573.15;

boundaryField
{
    inlet
    {
        type            isentropicTemperature;
        T0.1            uniform 573.15;
        p0              uniform 100000;
        value           uniform 573.15;
    }
    outlet
    {
        type            zeroGradient;
    }
    
    wall
    {
        type            zeroGradient;
    }

    periodic1
    {
        type            cyclic;
    }
    
    periodic2
    {
        type            cyclic;
    }

    emptyPlanes
    {
        type            empty;
    }
}


