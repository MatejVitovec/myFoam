FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        //type            fixedValue;
        type            fixedMean;
        value           uniform 42614.0;
        meanValue       42614.0;
    }

    wall
    {
        type            zeroGradient;
    }
    
    periodic1
    {
        type            cyclic;
    }
    
    periodic2
    {
        type            cyclic;
    }

    emptyPlanes
    {
        type            empty;
    }
}


