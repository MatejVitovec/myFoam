FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      U.1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (200 0 0);

boundaryField
{
    inlet
    {
        type            isentropicInletVelocity;
	    inletDirection  uniform (1 0 0);
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    wall
    {
        type            slip;
    }

    emptyPlanes
    {
        type            empty;
    }
}


