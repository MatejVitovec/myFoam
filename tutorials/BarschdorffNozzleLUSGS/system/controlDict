FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs (
  "libfluidThermophysicalModels.so"
  "libmyFoam.so"
);

application     wetSteamLusgsFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.1;

deltaT          1e-6;

writeControl    timeStep;

writeInterval   1000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  16;

writeCompression off;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable true;

maxCo 0.03;

adjustTimeStep yes;

//functions
//{
//    #includeFunc MachNo
//    #includeFunc axisLine
//}

functions
{
    relaxationLoss
    {
        type relaxationLoss;
        libs ("libmyfieldFunctionObjects.so");

        executeControl  writeTime;
        writeControl    writeTime;
    }
}

