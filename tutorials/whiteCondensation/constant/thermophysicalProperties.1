FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties.1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       IAPWSIF97;
    thermo          IAPWSIF97meta;
    equationOfState IAPWSIF97metaGas;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   18.0153;
    }
    thermodynamics
    {
        Cv          715.5;
        Hf          0;
	    Tref        0;
        Eref        0;
    }
    transport
    {
        mu           0;
        Pr           0.72;
    }
}

saturationCurve IF97;

condensationModel momentum;

//surfaceTensionCorrection 1.194;
Kantrowitz on;

liquidProperties H2O_IAPWS97reg1;

