startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1.25e-5;

deltaT          1.25e-9;

writeControl    timeStep;

writeInterval   500;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  no;

maxCo 0.3;

rDeltaTSmoothingCoeff 0.1;


