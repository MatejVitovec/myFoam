FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


regions
(
    boxToCell
    {
        box (0 0 0) (5 1 0.1);
        fieldValues
        (
	        volScalarFieldValue alpha 0.9999999
        );
    }
);





