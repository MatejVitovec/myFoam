FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

riemannSolver           hllc;

ddtSchemes
{
    default             Euler; 
}

gradSchemes
{
    default             Gauss linear;
    limitedGauss        cellLimited Gauss linear 1;
    venkatakrishnan     cellLimited<cubic> 1.5 edgeCellsLeastSquares 1;
}

divSchemes
{
    default             none;
    div(rhoPhi,w_Q)    Gauss multivariateIndependent
    {
        "Q.*" upwind;
    };

    div(rhoPhi,n)	Gauss upwind;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;

    reconstruct(p)  linearUpwind  venkatakrishnan;
    reconstruct(U)  linearUpwindV venkatakrishnan;
    reconstruct(T)  linearUpwind  venkatakrishnan;
    reconstruct(w)  linearUpwind  venkatakrishnan;

}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
}

