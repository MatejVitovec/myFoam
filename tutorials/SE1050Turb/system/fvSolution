FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    rho
    {
        solver diag;
    }

    rhoU
    {
        solver diag;
    }

    rhoE 
    {
        solver diag;
    }

    "(k|omega)"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-8;
        relTol          0;
        //solver          BiCCG;
	//preconditioner  DILU;
        //tolerance       1e-8;
        //relTol          0;
        minIter 1;
    }
}

relaxationFactors
{
  equations
    {
      "(k|epsilon|omega)"   0.7;
      yWall           0.7;
    }
}




