FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1e-10;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1e-5;
    }
    
    outlet
    {
        type            zeroGradient;
    }

    wall
    {
        type            slip;
    }

    periodic1
    {
        type            cyclic;
    }
    
    periodic2
    {
        type            cyclic;
    }
    
    emptyPlanes
    {
        type            empty;
    }
}

// ************************************************************************* //
