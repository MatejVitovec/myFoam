FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
    limitedGauss    cellLimited Gauss linear 1;
    venkatakrishnan cellLimited<cubic> 1.5 leastSquares 1;
    grad(U) Gauss linear;
    grad(h) Gauss linear;
}

divSchemes
{
    default         none;
    div(phi,k)      Gauss linearUpwind limitedGauss;
    div(phi,omega)  Gauss linearUpwind limitedGauss;
    div(devRhoReff) Gauss linear;
    div((devRhoReff&U)) Gauss linear;

    div((muEff*dev2(grad(U).T()))) Gauss linear;
}

laplacianSchemes
{
   default         Gauss linear limited corrected 0.5;
}

interpolationSchemes
{
    default        linear;
    //reconstruct(p)      upwind;
    //reconstruct(U)      upwind;
    //reconstruct(T)      upwind;

    reconstruct(p)      linearUpwind  venkatakrishnan;
    reconstruct(U)      linearUpwindV venkatakrishnan;
    reconstruct(T)      linearUpwind  venkatakrishnan;
}

snGradSchemes
{
    //default         corrected;
    default         limited corrected 0.5;
}

fluxRequired
{
    default         no;
}

wallDist {
  method meshWave;
}



